{{#if model.level}}
  <div class="screen_shot_level">
    {{model.level}}
  </div>
{{/if}}

{{#if model.name}}
  <span class="screen_shot_name">
    {{model.name}}
  </span>
{{else}}
  ERROR: missing name.
{{/if}}

{{ss-source source=model.source}}

{{screen-shot-img imgSrc=imgSrc toggleImage=toggleImage}}

{{yield}}
